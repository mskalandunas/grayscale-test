{
    "extends": "eslint-config-mosaic",
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "no-floating-decimal": 1
    }
}
